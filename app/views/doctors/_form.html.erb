<%= form_for @doctor do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name, required: true, autofocus: true, placeholder: "First Name" %>

     <%= f.label :last_name %>
    <%= f.text_field :last_name, required: true, autofocus: true, placeholder: "First Name" %>

    <%= f.submit%>
<% end %>